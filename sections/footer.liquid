<div class="banner-block banner-subsribe">
  <div class="container">

    <form class="form form-subscribe" action="/subscribe.php" method="post">
      <fieldset class="form-fieldset">
        <input type="hidden" name="action" value="subscribe">
        <input type="hidden" name="nl_first_name" value="bc">
        <input type="hidden" name="check" value="1">
        <div class="form-field">
          <label class="form-label" for="nl_email">
            Sign up for our emails!
          </label>
          <div class="form-prefixPostfix wrap subscribe-email-field">
            <input type="email" value="{% if customer %}{{ customer.email }}{% endif %}" placeholder="{{ 'general.newsletter_form.newsletter_placeholder' | t }}" name="contact[email]" id="Email" class="input-group-field newsletter__input form-input" autocorrect="off" autocapitalize="off">
            <input class="button button--primary form-prefixPostfix-button--postfix btn btn--small event_profile_email_signup" type="submit" value="JOIN" id="Subscribe">
          </div>
        </div>
      </fieldset>
    </form>
  </div>
</div>
<footer class="footer" role="contentinfo">
  <div class="container">
    <section class="footer__content">
      <div class="footer-logo footer-logo--center">
        <a href="#" class="event_menu_click" data-action-detail="footer : home">
          <img src="//cdn.shopify.com/s/files/1/0497/3733/4939/t/4/assets/logo.svg?v=6081629309748966955" alt="elogic logo" class="w100">
        </a>
      </div>
      <p>
        CONNECT WITH US
      </p>
      <ul class="footer-socials">
        <li>
          <a href="#" class="event_socialmedia_exit" data-action-detail="twitter" aria-label="Twitter" target="_blank" rel="noopener">
            <img src="https://cdn11.bigcommerce.com/s-rq4scv6wd6/product_images/uploaded_images/twitter-logo.png" alt="Twitter Logo">
          </a>
        </li>
        <li>
          <a href="#" class="event_socialmedia_exit" data-action-detail="facebook" aria-label="Facebook" target="_blank" rel="noopener">
            <img src="https://cdn11.bigcommerce.com/s-rq4scv6wd6/product_images/uploaded_images/fb-logo.png" alt="Facebook Logo">
          </a>
        </li>
        <li>
          <a href="#" class="event_socialmedia_exit" data-action-detail="instagram" aria-label="Instagram" target="_blank" rel="noopener">
            <img src="https://cdn11.bigcommerce.com/s-rq4scv6wd6/product_images/uploaded_images/ig-logo.png" alt="Instagram Logo">
          </a>
        </li>
        <li>
          <a href="#" class="event_socialmedia_exit" data-action-detail="youtube" aria-label="YouTube" target="_blank" rel="noopener">
            <img src="https://cdn11.bigcommerce.com/s-rq4scv6wd6/product_images/uploaded_images/youtube-logo.png" alt="Youtube Logo">
          </a>
        </li>
      </ul>
      <ul class="footer-menu">
        <li><a href="#">Contact Us</a></li>
        <li>
          <a href="#" class="event_menu_click" data-action-detail="footer : faq">FAQ</a>
        </li>
        <li><a href="#" class="event_internal_link" data-action-detail="Do not sell my personal information">Do not sell my personal information</a></li>
        <li><a href="#" class="event_outbound_link" data-action-detail="Terms &amp; Conditions" target="_blank" rel="noopener">Terms &amp; Conditions</a></li>
        <li><a href="#" class="event_outbound_link" data-action-detail="Privacy" target="_blank" rel="noopener">Privacy</a></li>
        <li><a class="event_outbound_link" title="Privacy Notice" data-action-detail="https://www.pg.com/privacy/english/privacy_statement.shtml#CCPA" href="#" target="_blank">CA Privacy</a></li>
      </ul>
      <a href="" class="btn-choises event_outbound_link" data-action-detail="AdChoices" target="_blank" rel="noopener">
        <div class="image">
          <img src="https://cdn11.bigcommerce.com/s-rq4scv6wd6/stencil/97951950-e574-0138-68f6-0242ac110007/e/9a4cd950-b892-0138-b26b-0242ac110008/img/icons/Secret_AdChoices_logo_footer_optimized.png" alt="AdChoise button">
        </div>
        <span>AdChoices</span>
      </a>
    </section>
    <div class="footer-copyright">
      <p class="powered-by">© 2020 Elogic </p>
    </div>
  </div>
</footer>


