@keyframes menuToggleTop {
  0% {
    left: 0;
  }

  50% {
    top: 30px;
    left: -50%;
  }

  75% {
    top: 30px;
    left: -100%;
  }

  100% {
    top: 30px;
  }
}

@keyframes menuToggleClose {
  0% {
    left: 0;
    width: 100%;
    height: 100%;
    position: fixed;
    background-color: #009ED8;
  }

  25% {
    left: -150px;
    width: 100%;
    height: 100%;
  }

  50% {
    left: -800px;
    position: fixed;
    width: 100%;
    height: 100%;
  }

  100% {
    left: -9999px;
    width: 0;
    height: 0;
    position: fixed;
    background-color: #009ED8;
  }
}

